{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "node",
    "promise",
    "security",
    "import",
    "@typescript-eslint"
  ],
  "rules": {
    "no-console": "off",
    "promise/always-return": "warn",
    "security/detect-object-injection": "off",
    "consistent-return": "warn",
    "import/extensions": ["off"]
  }
}
